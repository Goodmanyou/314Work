<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.Idle" table="Idle">
    
        <id name="idle_id" type="java.lang.Integer">
            <column name="idle_id" />
            <generator class="native" />
        </id>
        
        <property name="equ_cat" type="java.lang.String">
            <column name="Equ_cat" />
        </property>
        
        <property name="equ_name" type="java.lang.String">
        	<column name="Equ_name" />
        </property>
        
        <property name="equ_num" type="java.lang.Integer">
            <column name="Equ_num" />
        </property>
        
        <property name="idle_loc" type="java.lang.String">
            <column name="Idle_loc" />
        </property>
        

    </class>
</hibernate-mapping>
