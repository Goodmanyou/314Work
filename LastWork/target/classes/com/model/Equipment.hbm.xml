<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.Equipment" table="Equipment">
    
        <id name="equ_id" type="java.lang.Integer">
            <column name="Equ_id" />
            <generator class="native" />
        </id>
        
        <property name="equ_name" type="java.lang.String">
            <column name="Equ_name" />
        </property>
        
        <property name="equ_price" type="java.lang.Double">
            <column name="Equ_price" />
        </property>
        
        <property name="equ_cat" type="java.lang.String">
            <column name="Equ_cat" />
        </property>
        
        <property name="equ_model" type="java.lang.String">
            <column name="Equ_model" />
        </property>
        
        <property name="equ_fac" type="java.lang.String">
            <column name="Equ_fac" />
        </property>
        
        <property name="equ_sup" type="java.lang.String">
            <column name="Equ_sup" />
        </property>
        
        
    </class>
</hibernate-mapping>